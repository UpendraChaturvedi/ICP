class Solution {
    public int eraseOverlapIntervals(int[][] intervals) {
        Arrays.sort(intervals,(a,b)->a[1]-b[1]);
        int c=0;
        int n=intervals.length;
        int en=intervals[0][1];
        for(int i=1;i<n;i++){
            if(intervals[i][0]<en){
                c++;
            }
            en=intervals[i][1];
        }
        return c;
    }
}
