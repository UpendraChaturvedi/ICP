class Solution {
    public int lengthOfLongestSubstring(String s) {
        int n=s.length();
        int len=0;
        int i=0;
        HashSet<Character>h=new HashSet<>();
        for(int j=0;j<n;j++){
            while(h.contains(s.charAt(j))){
                h.remove(s.charAt(i));
                i++;
            }
            h.add(s.charAt(j));
            len=Math.max(len,j-i+1);
        }
        return len;
    }
}
