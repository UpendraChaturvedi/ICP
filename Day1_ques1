class Solution {
    public int findMinArrowShots(int[][] points) {
        Arrays.sort(points,(a,b)->Integer.compare(a[1],b[1]));
        int lastpos=points[0][1];
        int arr=1;
        int n=points.length;
        for(int i=1;i<n;i++){
            if(lastpos<points[i][0]){
                arr++;
                lastpos=points[i][1];
                
            }
        }
        return arr;
    }
}
